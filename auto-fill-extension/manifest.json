{
  "manifest_version": 3,
  "name": "Auto Fill Form",
  "version": "1.0",
  "description": "Auto-fill basic form fields like name, email, and phone.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}